{
  "$schema": "https://json-schema.org/draft/2025-05/schema",
  "$id": "https://api.elhub.no/schemas/contract-response-schema.json",
  "title": "Contract Response",
  "description": "Schema for a contract response in the Contracts JSON:API, compliant with JSON:API v1.0.",
  "type": "object",
  "properties": {
    "data": {
      "description": "The primary data, either a single contract or an array of contracts.",
      "oneOf": [
        { "$ref": "schema.json#/definitions/contract" },
        {
          "type": "array",
          "items": { "$ref": "schema.json#/definitions/contract" },
          "uniqueItems": true
        }
      ]
    },
    "meta": {
      "$ref": "meta-schema.json#/definitions/meta"
    },
    "links": {
      "description": "Links related to the contract response.",
      "$ref": "../base-definitions-schema.json#/definitions/topLevelLinks"
    }
  },
  "required": ["data"],
  "additionalProperties": false
}
